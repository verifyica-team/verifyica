<ruleset comparisonMethod="maven"
         xmlns="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://mojo.codehaus.org/versions-maven-plugin/rule/2.0.0 http://mojo.codehaus.org/versions-maven-plugin/xsd/rule-2.0.0.xsd">
    <rules>
        <rule groupId="org.junit.jupiter" artifactId="junit-jupiter">
            <ignoreVersions>
                <ignoreVersion type="regex">^6\..*</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.junit.jupiter" artifactId="junit-jupiter-api">
            <ignoreVersions>
                <ignoreVersion type="regex">^6\..*</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.junit.jupiter" artifactId="junit-jupiter-engine">
            <ignoreVersions>
                <ignoreVersion type="regex">^6\..*</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.junit.platform" artifactId="junit-platform-commons">
            <ignoreVersions>
                <ignoreVersion type="regex">^6\..*</ignoreVersion>
            </ignoreVersions>
        </rule>
        <rule groupId="org.junit.platform" artifactId="junit-platform-launcher">
            <ignoreVersions>
                <ignoreVersion type="regex">^6\..*</ignoreVersion>
            </ignoreVersions>
        </rule>
    </rules>
    <ignoreVersions>
        <ignoreVersion type="regex">.*[-_\.](alpha|Alpha|ALPHA|b|beta|Dev|Beta|BETA|rc|RC|M|EA|SNAPSHOT)[-_\.]?[0-9]*</ignoreVersion>
    </ignoreVersions>
</ruleset>